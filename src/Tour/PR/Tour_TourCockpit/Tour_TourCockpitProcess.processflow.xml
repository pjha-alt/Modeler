        <ReturnValues>
          <Return name="ProcessContext::VehicleDetailsTourBo" value="vehicleDetailsTourBo" />
        </ReturnValues>
        <TransitionTo action="CardTourStart_MergeVehicleDetailTourBo"/>
      </Action>
      <Action actionType="LOGIC" name="CardTourStart_MergeVehicleDetailTourBo" call="ProcessContext::TourDataBo.mergeProperties">
