          <Input name="docType" value="ProductCheckIn" />
        </Parameters>
        <Return name="ProcessContext::CardTourEnd_TourPendingCheckinDocuCount" />
        <TransitionTo action="CardTourEnd_getIsMandatoryCheckinCompleted"/>
      </Action>
      <Action name="CardTourEnd_getIsMandatoryCheckinCompleted" actionType="LOGIC" call="ProcessContext::TourDataBo.isMandatoryCheckinCompleted">
        <Parameters>
          <Input name="tourDocuments" value="ProcessContext::CardTourEnd_TourCheckinDocList" />
        </Parameters>
        <Return name="ProcessContext::CardTourEnd_isMandatoryCheckinCompleted" />
        <TransitionTo action="CardTourEnd_loadTourEndData"/>
