          <Input name="tourDataBo" value="ProcessContext::TourDataBo" />
          <Input name="checkinDocCount" value="ProcessContext::CardTourEnd_TourCheckinDocuCount" />
          <Input name="pendingCheckinDocCount" value="ProcessContext::CardTourEnd_TourPendingCheckinDocuCount" />
          <Input name="isMandatoryCheckinCompleted" value="ProcessContext::CardTourEnd_isMandatoryCheckinCompleted" />
        </Parameters>
        <Return name="ProcessContext::CardTourEnd_TourEndList" />
        <TransitionTo action="CardTourEnd_setDataLoaded" />
      </Action>
      <Action actionType="LOGIC" name="CardTourEnd_setDataLoaded" call="Utils.identity">
        <Return name="ProcessContext::CardTourEnd_DataLoaded" />
        <Parameters>
          <Input name="value" value="1" type="Literal" />
        </Parameters>
        <TransitionTo action="CardTourEnd_RefreshTourCockpitDecision" />
      </Action>
      <!-- This code checks if the data is being loaded after clicking the Start Tour Activities - Activities Completed button. If so, reset the flag and refresh the Tour Cockpit. -->
      <Action name="CardTourEnd_RefreshTourCockpitDecision" actionType="DECISION" parameter="ProcessContext::CardTourEnd_RefreshTourCockpit">
        <Case value="1" action="CardTourEnd_ResetRefreshTourCockpit"/>
      </Action>
      <Action actionType="LOGIC" name="CardTourEnd_checkTasksEnabled" call="ProcessContext::CardTourEnd_TourEndList.checkTasksEnabled">
        <Parameters>
          <Input name="visitsList" value="ProcessContext::cardTourVisits_TourVisitsList" />
          <Input name="startTourActivitiesCompleted" value="ProcessContext::TourDataBo.startTourActivitiesCompleted" />
        </Parameters>
        <Return name="ProcessContext::CardTourEnd_TasksEnabled" />
        <TransitionTo action="CardTourEnd_TasksDecision" />
      </Action>
      <Action name="CardTourEnd_TasksDecision" actionType="DECISION" parameter="ProcessContext::CardTourEnd_TasksEnabled">
        <Case value="Completed" action="CardTourEnd_Redirect"/>
      </Action>
      <Action actionType="DECISION" name="CardTourEnd_Redirect" parameter="Event.activityId">
        <Case value="vehicleDetails" action="CardTourEnd_ShowVehicleDetails" />
        <Case value="checkinInventory" action="CardTourEnd_ShowCheckinDocuments" />
        <Case value="vehicleInspection" action="CardTourEnd_ShowVehicleInspection" />
        <Case value="safetyInspection" action="CardTourEnd_ShowSafetyInspection" />
        <CaseElse action="ShowTourCockpitUI" />
      </Action>
      <Action actionType="PROCESS" name="CardTourEnd_ShowVehicleDetails" process="Tour::TourDetailsProcess">
        <Parameters>
          <Input name="TourId" value="ProcessContext::TourDataBo.pKey" />
          <Input name="InitialTabName" value="ReviewVehicleDetails" type="Literal" />
          <Input name="InvokedFrom" value="EndTourCard" type="Literal" />
          <Input name="TourVisitsList" value="ProcessContext::CardMap_TourVisitsList" />
        </Parameters>
        <ReturnValues>
          <Return name="ProcessContext::VehicleDetailsTourBo" value="vehicleDetailsTourBo" />
        </ReturnValues>
        <TransitionTo action="CardTourEnd_MergeVehicleDetailTourBo"/>
      </Action>
      <Action actionType="LOGIC" name="CardTourEnd_MergeVehicleDetailTourBo" call="ProcessContext::TourDataBo.mergeProperties">
        <Parameters>
           <Input name="vehicleDetailsTourBo" value="ProcessContext::VehicleDetailsTourBo" />
        </Parameters>
        <TransitionTo action="CardTourEnd_loadTourCheckinDocList"/>
      </Action>
      <Action actionType="PROCESS" name="CardTourEnd_ShowVehicleInspection" process="Tour::TourDetailsProcess">
        <Parameters>
          <Input name="TourId" value="ProcessContext::TourDataBo.pKey" />
          <Input name="InitialTabName" value="VehicleInspection" type="Literal" />
          <Input name="InvokedFrom" value="EndTourCard" type="Literal" />
          <Input name="TourVisitsList" value="ProcessContext::CardMap_TourVisitsList" />
        </Parameters>
        <ReturnValues>
          <Return name="ProcessContext::VehicleDetailsTourBo" value="vehicleDetailsTourBo" />
        </ReturnValues>
        <TransitionTo action="CardTourEnd_MergeVehicleDetailTourBo"/>
      </Action>
      <Action actionType="PROCESS" name="CardTourEnd_ShowSafetyInspection" process="Tour::TourDetailsProcess">
        <Parameters>
          <Input name="TourId" value="ProcessContext::TourDataBo.pKey" />
          <Input name="InitialTabName" value="SafetyInspection" type="Literal" />
          <Input name="InvokedFrom" value="EndTourCard" type="Literal" />
          <Input name="TourVisitsList" value="ProcessContext::CardMap_TourVisitsList" />
        </Parameters>
        <ReturnValues>
          <Return name="ProcessContext::VehicleDetailsTourBo" value="vehicleDetailsTourBo" />
        </ReturnValues>
        <TransitionTo action="CardTourEnd_MergeVehicleDetailTourBo"/>
      </Action>
      <Action actionType="PROCESS" name="CardTourEnd_ShowCheckinDocuments" process="Tour::TourDocumentsProcess">
        <Parameters>
          <Input name="tourDataBo" value="ProcessContext::TourDataBo" />
        </Parameters>
      </Action>
      <!-- End Tour -->
      <Action name="checkPendingMandatoryEndTourActivities" actionType="LOGIC" call="ProcessContext::CardTourEnd_TourEndList.getPendingMandatoryActivities">
        <Return name="ProcessContext::PendingMandatoryEndTourActivities" />
        <TransitionTo action="EndTour" />
      </Action>
      <Action name="EndTour" actionType="LOGIC" call="ProcessContext::TourDataBo.endTour">
        <Parameters>
          <Input name="isMandatoryCheckinCompleted" value="ProcessContext::CardTourEnd_isMandatoryCheckinCompleted" />
          <Input name="isPendingMandatoryEndTourActivities" value="ProcessContext::PendingMandatoryEndTourActivities" />
        </Parameters>
        <Return name="ProcessContext::IsEndTourActivitiesPending" />
        <TransitionTo action="EndTour_Completed" />
      </Action>
      <Action name="EndTour_Completed" actionType="DECISION" parameter="ProcessContext::IsEndTourActivitiesPending">
        <Case value="Completed" action="end" />
      </Action>
      <!-- End Tour End -->
      <Action actionType="END" name="end" />
    </Actions>
  </Body>
</Process>